{
	"name": "VBX Browser Phone",
	"short_name": "Browser Phone",
	"description": "Enables a Twilio or Plivo Client",
	"version": "2019.01.13",
	"manifest_version": 2,
	"minimum_chrome_version": "40.0.0",
	"homepage_url": "http://vbx.io/browser-phone",
	"offline_enabled": false,
	"incognito": "not_allowed",
	"icons": {
		"16":"img/icon16.png",
		"48":"img/icon48.png",
		"128":"img/icon128.png"
	},
	"content_security_policy": "media-src 'self' https://media.twiliocdn.com https://eventgw.twilio.com; object-src 'self'; script-src 'self' 'unsafe-eval' https://media.twiliocdn.com https://eventgw.twilio.com https://cdn.plivo.com;",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": {
			"16": "img/icon16.png",
			"24": "img/icon24.png",
			"32": "img/icon32.png",
			"48": "img/icon48.png",
			"128": "img/icon128.png"
		},
		"default_popup": "popup.html",
		"default_title": "Browser Phone"
	},
	"options_page": "options.html",
	"permissions": [
		"contextMenus"
	]
}
